    field(DESC, "Timing estimated Time Delta")
    field(EGU,  "ms")
    field(INPA, "$(TIM)Duration-RB CPP NMS")
    field(CALC, "A/1000")
    field(OOPT, "Every Time")
    field(OUT,  "$(P)$(R)SpecAnaTDelta-Mon PP")
}

