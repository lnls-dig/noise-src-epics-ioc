# AUTOSAVE
#
# Desc: autosave.

record(bo, "$(P)$(R)Save-Cmd"){
  field(DESC, "Manual 'Save Settings' command")
  field(MASK, "1")
  field(ZNAM, "Dsbl")
  field(ONAM, "Enbl")
  field(HIGH, "1")
}
